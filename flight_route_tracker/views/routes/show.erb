<h2>Route details</h2>
<h3>Origin:</h3> <%=@route.origin%>
<h3>Destination:</h3><%=@route.destination%>
<h3>Price:</h3><%=@route.price%>
<h3>Currency:</h3><%=@route.currency%>

<form action="/flight-routes/<%=@route.id%>/delete" method="POST">
  <input type="submit" value="REMOVE Route">
</form>

<form action="/flight-routes/<%=@route.id%>/deals">
  <input type="submit" value="Available DEALS">
</form>

<form action="/flight-routes/<%=@route.id%>/airlines">
  <input type="submit" value="List AIRLINES">
</form>
